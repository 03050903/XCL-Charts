package org.xclcharts.renderer.info;

public class ToolTip extends DyInfo{
	
	public ToolTip()
	{
		super();
	}
	
	
	public void showRectHint()
	{
		mStyle = 0;
	}
	
	public void showRoundRectHint()
	{
		mStyle = 1;
	}
	
	public void showRoundRectHint(float x,float y)
	{
		mStyle = 1;
		mRoundRectX = x;
		mRoundRectY = y;
	}
	

}
