package org.xclcharts.chart;



public class FunnelData {
	
	private String mLabel;
	private String mKey;
	
//	private List<Double> mDataSet;
	//private List<Integer> mAreaColor;
	
	
	private double mData;
	private int mColor;
	
	public FunnelData()
	{
		
	}
	
	/**
	 * 设置数据源
	 * @param dataSeries 数据集合序列
	 */
	public void setDataSet(double data) //List<Double> dataSeries) 
	{
		//if(null != mDataSet)mDataSet.clear();
	//	mDataSet = dataSeries;
		
		mData = data;
	}
	
	
	/**
	 * 设置Key值
	 * @param value Key值
	 */
	public void setKey(String value) 
	{
		mKey = value;
	}
	
	/**
	 * 设置颜色
	 * @param value 颜色
	 */
	public void setColor(int value) 
	{
		mColor = value;
	}

	/**
	 * 返回数据集合序列
	 * @return 集合序列
	 */
	public double  getDataSet() { //List<Double>
		return mData;
	}


	/**
	 * 返回Key值
	 * @return Key值
	 */
	public String getKey() {
		return mKey;
	}

	

}
