package org.xclcharts.renderer.plot;

import org.xclcharts.renderer.XEnum;

import android.graphics.Paint;

public class LabelLine {
	
	private XEnum.LabelLinePoint mLinePoint = XEnum.LabelLinePoint.NONE;
	private float mRadius = 10.f;
	
	//当标签为Line类型时使用
	private Paint mPaintLabelLine = null;
		
	
	public LabelLine()
	{		
	}

	public void setLinePoint(XEnum.LabelLinePoint linePoint)
	{
		mLinePoint = linePoint;
	}
	
	public void setRadius(float radius)
	{
		mRadius = radius;
	}
	
	
	
	
}
